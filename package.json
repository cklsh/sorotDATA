  "private": true,
  "dependencies": {
    "@typed/compose": "^1.0.0",
    "@types/googlemaps": "^3.30.7",
    "@types/jquery": "^3.3.0",
    "@types/lodash": "^4.14.105",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "7.0.7",
    "@types/react-icons": "^2.2.5",
    "@types/react-redux": "^5.0.15",
    "@types/react-sidebar": "^2.2.3",
    "@types/reactstrap": "^5.0.17",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "cross-env": "^5.1.3",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "sweetalert2": "^7.13.3",
    "ts-loader": "^4.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/assets/sass/index.sass --include-path ./node_modules src/assets/sass/index.sass -o src/assets/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/assets/sass/index.sass --include-path ./node_modules src/assets/sass/index.sass -o src/assets/ --watch --recursive",
    "start-ts": "cross-env NODE_PATH=src/ react-scripts-ts start",
    "build-ts": "cross-env NODE_PATH=src/ react-scripts-ts build",
    "start": "npm run watch-css | npm run start-ts",
    "build": "npm run build-css | npm run build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "datatables.net": "^1.10.16",
    "jquery": "^3.3.1"
  }
}
